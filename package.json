{
  "name": "gastos-gub-scraper",
  "version": "1.0.0",
  "description": "A TypeScript web scraper following SOLID principles to extract government expenditure data URLs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "analyze": "tsx src/analyzer.ts",
    "schema": "tsx src/schema-only.ts",
    "upload": "tsx src/upload-releases.ts",
    "test-upload": "tsx src/test-upload.ts",
    "clear-db": "tsx src/clear-database.ts",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "web-scraping",
    "solid-principles",
    "government-data",
    "uruguay"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.0.0",
    "@types/stream-json": "^1.7.8",
    "@types/yauzl": "^2.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@types/mongodb": "^4.0.6",
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.0",
    "stream-json": "^1.9.1",
    "yauzl": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
