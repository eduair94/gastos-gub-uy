{
  "name": "gastos_gub-scraper",
  "version": "1.0.0",
  "description": "A TypeScript web scraper following SOLID principles to extract government expenditure data URLs",
  "main": "dist/index.js",
  "scripts": {
    "extract": "tsx src/extract.ts",
    "upload": "tsx src/upload-releases.ts",
    "build": "tsc",
    "create-items": "tsx src/create-items-collection.ts",
    "api": "tsx src/api/app.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "web-scraping",
    "solid-principles",
    "government-data",
    "uruguay"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.0.0",
    "@types/stream-json": "^1.7.8",
    "@types/yauzl": "^2.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@types/mongodb": "^4.0.6",
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "helmet": "^7.1.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.0",
    "mongoose-aggregate-paginate-v2": "^1.1.0",
    "mongoose-paginate-v2": "^1.8.0",
    "stream-json": "^1.9.1",
    "yauzl": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
